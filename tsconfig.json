{
    "compilerOptions": {
        "target": "es5",
        "jsx": "react",
        "strict": true,
        "alwaysStrict": true,
        "skipLibCheck": true,
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "esModuleInterop": true,
        "noFallthroughCasesInSwitch": true,
        "baseUrl": "./",
    },
    "exclude": [
        "node_modules"
    ]
}